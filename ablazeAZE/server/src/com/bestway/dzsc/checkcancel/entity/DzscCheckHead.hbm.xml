<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" 
		name="com.bestway.dzsc.checkcancel.entity.DzscCheckHead" 
		table="dzsccheckhead">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="emsNo" length="20"/>
		<property name="tradeCode" length="100"/>
		<property name="tradeName" length="255"/>
		<property name="machCode" length="100"/>
		<property name="machName" length="255"/>
		<property name="beginDate"/>
		<property name="isDeclare"/>
		<property name="copEmsNo"/>
		<property name="colRange"/>
		<!--<property name="copTrNo"/>-->
		<property name="declareState"/>
		<property name="colShdulTime"/>
		<property name="ctockData"/>
		<property name="declareDate"/>
		<property name="flagHG"/>
		<property name="manageObject"/>
		<many-to-one lazy="false" name="masterCustoms" column="masterCustoms" 
			class="com.bestway.bcus.custombase.entity.countrycode.Customs" />
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" 
			index="dzsccheckhead_coid"/>
	</class>

</hibernate-mapping>
