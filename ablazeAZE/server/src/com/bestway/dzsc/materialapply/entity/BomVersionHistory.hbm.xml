<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.dzsc.materialapply.entity.BomVersionHistory" table="bomversionhistory">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="bomVersionId"/>	
		<many-to-one lazy="false"      name="master" column="masterId" 
			class="com.bestway.common.materialbase.entity.MaterialBomMaster"/>
		<property name="version" length="1"/>	
		<property name="beginDate"/>
		<property name="endDate"/>
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company"/>
    </class>

</hibernate-mapping>
