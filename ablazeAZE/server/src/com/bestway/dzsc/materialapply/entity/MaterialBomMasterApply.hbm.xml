<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.dzsc.materialapply.entity.MaterialBomMasterApply" table="materialbomapply">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>	
		<property name="isChanged" />
		<property name="stateMark" />
		<many-to-one lazy="false"      name="materiel" column="materiel" 
			class="com.bestway.common.materialbase.entity.Materiel" index="mbmaster_materiel"/>
		<many-to-one lazy="false" name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="mbmaster_coid"/>
    </class>

</hibernate-mapping>
