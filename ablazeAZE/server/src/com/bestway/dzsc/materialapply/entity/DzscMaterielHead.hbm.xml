<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" 
		name="com.bestway.dzsc.materialapply.entity.DzscMaterielHead" 
		table="dzscmaterielhead">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="copEntNo"/>
		<property name="tradeCode"/>
		<property name="tradeName"/>
		<property name="machCode"/>
		<property name="machName"/>
		<many-to-one lazy="false" name="masterCustoms" 
			class="com.bestway.bcus.custombase.entity.countrycode.Customs"/>
		<property name="inputDate"/>
		<property name="declareDate"/>
		<property name="manageObject"/>
		<property name="materialState"/>
		<property name="innerMergeState"/>
		<property name="bomState"/>
		<property name="note"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company"/>
	</class>

</hibernate-mapping>
<!-- parsed in 0ms -->
<!--country 指对象，countryCode 定义字段-->
