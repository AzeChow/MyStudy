<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.dzsc.materialapply.entity.MaterialBomVersionApply" table="materialbomversionapply">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>	
		<many-to-one lazy="false" name="bomMasterApply" column="bommasterapply" class="com.bestway.dzsc.materialapply.entity.MaterialBomMasterApply"/>
		<!--<property name="stateMark" index="index_state"/>
		<property name="isChanged" />-->
		<property name="version" index="version"/>	
		<property name="beginDate"/>
		<property name="endDate"/>
		<many-to-one lazy="false" name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="mbversion_coid"/>
    </class>

</hibernate-mapping>
