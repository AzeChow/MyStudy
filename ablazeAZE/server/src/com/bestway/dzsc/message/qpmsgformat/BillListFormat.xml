<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="eportmsg.xsl"?>
<!--Element message: 此元素内所有内容及属性描述一个报文的定义及从数据库的获取途径。-->
<message version="1.0" type="MaterielApply" description="物料表头" data-id="BillListId">
	<field nullable="false">
		<description>商品货号（料号）</description>
		<field-name>materiel.ptNo</field-name>
		<format>X(30)</format>
		<field-flag>Cop_G_No</field-flag>
	</field>
	<field nullable="true">
		<description>币制</description>
		<field-name>currency.code</field-name>
		<format>X(16)</format>
		<field-flag>Curr</field-flag>
	</field>
	<field nullable="false">
		<description>申报数量（进出口数量）</description>
		<field-name>declaredAmount</field-name>
		<format>Z(9)9.9(4)</format>
		<field-flag>Qty</field-flag>
	</field>
	<field nullable="true">
		<description>企业申报单价</description>
		<field-name>declaredPrice</field-name>
		<format>Z(7)9.9(5)</format>
		<field-flag>Dec_Price</field-flag>
	</field>
	<field nullable="true">
		<description>企业申报总价</description>
		<field-name>totalPrice</field-name>
		<format>Z(8)9.9(4)</format>
		<field-flag>Dec_Total</field-flag>
	</field>
	<field nullable="true">
		<description>毛重</description>
		<field-name>grossWeight</field-name>
		<format>Z(8)9.9(4)</format>
		<field-flag>Gross_Wt</field-flag>
	</field>
	<field nullable="true">
		<description>净重</description>
		<field-name>netWeight</field-name>
		<format>Z(8)9.9(4)</format>
		<field-flag>Wet_Wt</field-flag>
	</field>
	<field nullable="true">
		<description>用途代码</description>
		<field-name>usesCode.code</field-name>
		<format>X(9)</format>
		<field-flag>Use_Type</field-flag>
	</field>
	<field nullable="true">
		<description>征免方式</description>
		<field-name>levyMode.code</field-name>
		<format>X(1)</format>
		<field-flag>Duty_Mode</field-flag>
	</field>
	<field nullable="true">
		<description>产销国(地区)</description>
		<field-name>salesCountry.code</field-name>
		<format>X(16)</format>
		<field-flag>Country_Code</field-flag>
	</field>
	<field nullable="true">
		<description>成品版本号</description>
		<field-name>bomVersion</field-name>
		<format>Z(9)</format>
		<field-flag>Exg_Version</field-flag>
	</field>
</message>
