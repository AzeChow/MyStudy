<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="eportmsg.xsl"?>
<!--Element message: 此元素内所有内容及属性描述一个报文的定义及从数据库的获取途径。-->
<message version="1.0" type="MaterielApply">
	<description>回执报文定义及获取方法描述</description>
	<section-flag>PTS_DATA</section-flag>
	<sub-section>
		<description>签名信息</description>
		<file-name>PtsSignFormat.xml</file-name>
	</sub-section>
	
	<sub-section>
		<description>物料图片表表头</description>
		<file-name>MaterielHeadFormat.xml</file-name>
	</sub-section>
	
	<section multiple="false" required="true">
		<description>企业物料图片表体</description>
		<section-flag>PTS_ENT_PIC_LIST</section-flag>
		
		<section multiple="false" required="true">
			<description>企业归并前料件图片表体</description>
			<section-flag>PTS_ENT_PIC_LIST_IMG</section-flag>
				<section multiple="false" required="true">
					<description>企业物料归并前料件图片</description>
					<section-flag>PTS_ENT_PIC_IMG</section-flag>
					<data-id></data-id>
					<field nullable="false" signNo="1">
						<description>料件/成品货号</description>
						<field-name></field-name>
						<format>X(30)</format>
						<field-flag>COP_G_NO</field-flag>
					</field>
					<field nullable="false" signNo="2">
						<description>图片序号</description>
						<field-name></field-name>
						<format>Z(2)</format>
						<field-flag>PIC_NO</field-flag>
					</field>
					<field nullable="false" signNo="3">
						<description>图片</description>
						<field-name></field-name>
						<format>X(128*1024)</format>
						<field-flag>G_PIC</field-flag>
					</field>
					<field nullable="true" signNo="4">
						<description>图片类型</description>
						<field-name></field-name>
						<format>X(1)</format>
						<field-flag>PIC_TYPE</field-flag>
					</field>
					<field nullable="false" signNo="5">
						<description>修改标志</description>
						<field-name></field-name>
						<format>X(1)</format>
						<field-flag>MODIFY_MARK</field-flag>
					</field>
				</section>
		</section>
		
		<section multiple="false" required="true">
			<description>企业归并前成品图片表体</description>
			<section-flag>PTS_ENT_PIC_LIST_EXG</section-flag>
				<section multiple="false" required="true">
					<description>企业物料归并前成品图片</description>
					<section-flag>PTS_ENT_PIC_EXG</section-flag>
					<data-id></data-id>
					<field nullable="false" signNo="1">
						<description>料件/成品货号</description>
						<field-name></field-name>
						<format>X(30)</format>
						<field-flag>COP_G_NO</field-flag>
					</field>
					<field nullable="false" signNo="2">
						<description>图片序号</description>
						<field-name></field-name>
						<format>Z(2)</format>
						<field-flag>PIC_NO</field-flag>
					</field>
					<field nullable="false" signNo="3">
						<description>图片</description>
						<field-name></field-name>
						<format>X(128*1024)</format>
						<field-flag>G_PIC</field-flag>
					</field>
					<field nullable="true" signNo="4">
						<description>图片类型</description>
						<field-name></field-name>
						<format>X(1)</format>
						<field-flag>PIC_TYPE</field-flag>
					</field>
					<field nullable="false" signNo="5">
						<description>修改标志</description>
						<field-name></field-name>
						<format>X(1)</format>
						<field-flag>MODIFY_MARK</field-flag>
					</field>
				</section>
		</section>
		
	</section>
</message>
