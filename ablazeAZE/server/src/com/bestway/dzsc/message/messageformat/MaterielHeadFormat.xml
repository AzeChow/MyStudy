<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="eportmsg.xsl"?>
<!--Element message: 此元素内所有内容及属性描述一个报文的定义及从数据库的获取途径。-->
<message version="1.0" type="MaterielApply">
	<section multiple="false" required="true">
		<description>物料表头</description>
		<section-flag>PTS_ENT_HEAD</section-flag>
		<data-id>MaterialHeadId</data-id>
		<field nullable="false" signNo="1">
			<description>企业内部编号</description>
			<field-name>copEntNo</field-name>
			<format>X(20)</format>
			<field-flag>COP_ENT_NO</field-flag>
		</field>
		<field nullable="false" signNo="2">
			<description>经营单位编码</description>
			<field-name>tradeCode</field-name>
			<format>X(10)</format>
			<field-flag>TRADE_CODE</field-flag>
		</field>
		<field nullable="false" signNo="3">
			<description>经营单位名称</description>
			<field-name>tradeName</field-name>
			<format>X(30)</format>
			<field-flag>TRADE_NAME</field-flag>
		</field>
		<field nullable="false" signNo="4">
			<description>加工单位编码</description>
			<field-name>machCode</field-name>
			<format>X(10)</format>
			<field-flag>OWNER_CODE</field-flag>
		</field>
		<field nullable="false" signNo="5">
			<description>加工单位名称</description>
			<field-name>machName</field-name>
			<format>X(30)</format>
			<field-flag>OWNER_NAME</field-flag>
		</field>
		<field nullable="false" signNo="6">
			<description>主管海关</description>
			<field-name>masterCustoms.code</field-name>
			<format>X(4)</format>
			<field-flag>MASTER_CUSTOMS</field-flag>
		</field>
		<field nullable="false" signNo="8">
			<description>录入日期</description>
			<field-name>inputDate</field-name>
			<format>Z(8)</format>
			<field-flag>INPUT_DATE</field-flag>
		</field>
		<field nullable="false" signNo="9">
			<description>申报日期</description>
			<field-name>declareDate</field-name>
			<format>Z(8)</format>
			<field-flag>DECLARE_DATE</field-flag>
		</field>
		<field nullable="false" signNo="7">
			<description>管理对象</description>
			<field-name>manageObject</field-name>
			<format>X(1)</format>
			<field-flag>MANAGE_OBJECT</field-flag>
		</field>
		<field nullable="true" signNo="10">
			<description>备注</description>
			<field-name>note</field-name>
			<format>X(50)</format>
			<field-flag>NOTE</field-flag>
		</field>
	</section>
</message>
