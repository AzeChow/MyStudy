<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="eportmsg.xsl"?>
<!--Element message: 此元素内所有内容及属性描述一个报文的定义及从数据库的获取途径。-->
<message version="1.0" type="MaterielApply">
    <section multiple="false" required="true">
        <description>签名信息</description>
        <section-flag>PTS_SIGN</section-flag>
		<data-id>PtsSignInfo</data-id>
		<field nullable="false">
            <description>企业编码</description>
            <field-name>tradeCode</field-name>
            <format>X(10)</format>
			<field-flag>TRADE_CODE</field-flag>
        </field>
        <field nullable="false">
            <description>企业内部编号</description>
            <field-name>copNo</field-name>
            <format>X(20)</format>
			<field-flag>COP_NO</field-flag>
        </field>
		<field nullable="false">
            <description>业务类型</description>
            <field-name>sysType</field-name>
            <format>X</format>
			<field-flag>SYS_TYPE</field-flag>
        </field> 
		<field nullable="false">
            <description>申报类型</description>
            <field-name>declareType</field-name>
            <format>X</format>
			<field-flag>DECLARE_TYPE</field-flag>
        </field> 
		<field nullable="false">
            <description>核查报核次数</description>
            <field-name>colDcrTime</field-name>
            <format>Z(8)9</format>
			<field-flag>COL_DCR_TIME</field-flag>
        </field> 
		<field nullable="false">
            <description>证书号</description>
            <field-name>certSeqNo</field-name>
            <format>X(20)</format>
			<field-flag>CERT_SEQ_NO</field-flag>
        </field> 
		<field nullable="false">
            <description>签名人IC卡ID</description>
            <field-name>idCard</field-name>
            <format>X(20)</format>
			<field-flag>ID_CARD</field-flag>
        </field>
		<field nullable="false">
            <description>签名时间</description>
            <field-name>signDate</field-name>
            <format>Z(14)</format>
			<field-flag>SIGN_DATE</field-flag>
        </field> 
		<field nullable="false">
            <description>签名</description>
            <field-name>sign</field-name>
            <format>X(380)</format>
			<field-flag>SIGN</field-flag>
        </field> 
		<field nullable="true">
            <description>备注</description>
            <field-name>note</field-name>
            <format>X(32)</format>
			<field-flag>NOTE</field-flag>
        </field> 
		<field nullable="true">
            <description>备用字段1</description>
            <field-name>note1</field-name>
            <format>X(32)</format>
			<field-flag>NOTE_1</field-flag>
        </field> 
		<field nullable="true">
            <description>备用代码1</description>
            <field-name>noteCode1</field-name>
            <format>X(32)</format>
			<field-flag>NOTE_CODE_1</field-flag>
        </field> 
		<field nullable="true">
            <description>备用代码2</description>
            <field-name>noteCode2</field-name>
            <format>X(32)</format>
			<field-flag>NOTE_CODE_2</field-flag>
        </field> 
    </section>
</message>
