<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.dzsc.message.entity.DzscReceiptResult" table="dzscreceiptresult">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="tradeCode"/>
		<property name="copEmsNo"/>
		<property name="seqNo"/>
		<property name="colDcrTime" />
		<property name="emsNo"/>
		<property name="applType"/>
		<property name="declareType"/>
		<property name="chkMark"/>
		<property name="noticeDate"/>
		<property name="noticeTime"/>								
		<property name="note"/>
		<property name="userName"/>		
		<property name="fileName" />
		<property name="sendFileName" />
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="receiptresult_company"/>
	</class>

</hibernate-mapping>
