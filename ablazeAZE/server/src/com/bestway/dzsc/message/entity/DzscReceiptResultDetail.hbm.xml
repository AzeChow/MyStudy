<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" 
		name="com.bestway.dzsc.message.entity.DzscReceiptResultDetail" 
		table="dzscreceiptresultdetail">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false" name="cspReceiptResult" 
			column="dzscReceiptResult" 
			class="com.bestway.dzsc.message.entity.DzscReceiptResult"/>
		<property name="resultInfo" />
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" 
			index="receiptresultd_company"/>
	</class>

</hibernate-mapping>
