			<field-name>inTradeCode</field-name>
			<format>X(10)</format>
			<field-flag>IN_TRADE_CODE</field-flag>
		</field>
		<field nullable="false" signNo="19">
			<description>承揽企业名称</description>
			<field-name>inTradeName</field-name>
			<format>X(30)</format>
			<field-flag>IN_TRADE_NAME</field-flag>
		</field>
		<field nullable="true" signNo="20">
			<description>承揽方联系电话</description>
			<field-name>inCorp</field-name>
			<format>X(32)</format>
			<field-flag>IN_CORP</field-flag>
		</field>
		<field nullable="false" signNo="21">
			<description>承揽方地区代码（目的地）</description>
			<field-name>inDistrict</field-name>
			<format>X(5)</format>
			<field-flag>IN_DIST_CODE</field-flag>
		</field>
		<field nullable="false" signNo="22">
			<description>申报类型</description>
			<field-name>declareType</field-name>
			<format>X(1)</format>
			<field-flag>DECLARE_TYPE</field-flag>
