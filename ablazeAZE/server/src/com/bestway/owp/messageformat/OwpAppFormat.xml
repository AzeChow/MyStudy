		<section-flag>FPT2_APP_OUT_HEAD</section-flag>
		<data-id>OwpAppHead</data-id>
		
		<field nullable="true" signNo="1">
			<description>申请表编号</description>
			<field-name>appNo</field-name>
			<format>X(12)</format>
			<field-flag>APP_NO</field-flag>
		</field>
		<field nullable="true" signNo="2">
			<description>电子口岸统一编号</description>
			<field-name>seqNo</field-name>
			<format>X(18)</format>
			<field-flag>SEQ_NO</field-flag>
		</field>
		<field nullable="false" signNo="2">
			<description>企业代码</description>
			<field-name>inTradeCode</field-name>
			<format>X(10)</format>
			<field-flag>TRADE_CODE</field-flag>
		</field>
