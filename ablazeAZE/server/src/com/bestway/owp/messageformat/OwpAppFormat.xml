			<description>委托方手册/账册编号</description>
			<field-name>emsNo</field-name>
			<format>X(12)</format>
			<field-flag>EMS_NO</field-flag>
		</field>
		<field nullable="false" signNo="7">
			<description>委托方企业代码</description>
			<field-name>tradeCode</field-name>
			<format>X(10)</format>
			<field-flag>TRADE_CODE</field-flag>
		</field>
		<field nullable="false" signNo="8">
			<description>委托方企业名称</description>
			<field-name>tradeName</field-name>
			<format>X(30)</format>
			<field-flag>TRADE_NAME</field-flag>
