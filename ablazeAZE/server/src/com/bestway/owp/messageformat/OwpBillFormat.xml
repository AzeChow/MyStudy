<?xml version="1.0" encoding="UTF-8"?>
<message version="1.0" type="FptBill">
	<description>外发加工登记表报文定义及获取方法描述</description>
	<section-flag>FPT2_DATA</section-flag>
	<sub-section>
		<description>签名信息</description>
		<file-name>OwpBillSignFormat.xml</file-name>
	</sub-section>
	<section multiple="false" required="true">
		<description>外发加工收发货单表头</description>
		<section-flag>FPT2_BILL_HEAD</section-flag>
		<data-id>OwpBillSendHead</data-id>
		
		<field nullable="true" signNo="1">
			<description>收发货单编号</description>
			<field-name>billNo</field-name>
			<format>X(17)</format>
			<field-flag>BILL_NO</field-flag>
		</field>
		<field nullable="true" signNo="2">
			<description>电子口岸统一编号</description>
			<field-name>seqNo</field-name>
			<format>X(18)</format>
			<field-flag>SEQ_NO</field-flag>
		</field>
		<field nullable="false" signNo="3">
			<description>申请表编号</description>
			<field-name>appNo</field-name>
			<format>X(12)</format>
			<field-flag>APP_NO</field-flag>
		</field>
		<field nullable="false" signNo="4">
			<description>发（收）货企业内部编号</description>
			<field-name>copBillNo</field-name>
			<format>X(20)</format>
			<field-flag>COP_BILL_NO</field-flag>
		</field>
		<field nullable="false" signNo="5">
			<description>帐册号</description>
			<field-name>emsNo</field-name>
			<format>X(12)</format>
			<field-flag>EMS_NO</field-flag>
		</field>
		<field nullable="false" signNo="6">
			<description>委托方企业编码</description>
			<field-name>outTradeCode</field-name>
			<format>X(10)</format>
			<field-flag>OUT_TRADE_CODE</field-flag>
		</field>
		<field nullable="false" signNo="7">
			<description>委托方企业名称</description>
			<field-name>outTradeName</field-name>
			<format>X(70)</format>
			<field-flag>OUT_TRADE_NAME</field-flag>
		</field>
		<field nullable="false" signNo="8">
			<description>承揽方企业编码</description>
			<field-name>inTradeCode</field-name>
			<format>X(10)</format>
			<field-flag>IN_TRADE_CODE</field-flag>
		</field>
		<field nullable="false" signNo="9">
			<description>承揽方企业名称</description>
			<field-name>inTradeName</field-name>
			<format>X(70)</format>
			<field-flag>IN_TRADE_NAME</field-flag>
		</field>
		<field nullable="false" signNo="10">
			<description>发（收）货申报人</description>
			<field-name>isDeclaEm</field-name>
			<format>X(10)</format>
			<field-flag>IS_DECLA_EM</field-flag>
		</field>
		<field nullable="false" signNo="11">
			<description>发(收) 货日期</description>
			<field-name>collectDate</field-name>
			<format>Z(8)</format>
			<field-flag>COLLECT_DATE</field-flag>
		</field>
		<field nullable="true" signNo="12">
			<description>备注</description>
			<field-name>note</field-name>
			<format>X(128)</format>
			<field-flag>NOTE</field-flag>
		</field>
	</section>

	<section multiple="false" required="true">
		<description>登记表表体发货部分</description>
		<section-flag>FPT2_BILL_ITEM</section-flag>
		<sub-section>
			<description>外发 登记表外发货物</description>
			<file-name>OwpBillSendItemFormat.xml</file-name>
		</sub-section>
	</section>
</message>
