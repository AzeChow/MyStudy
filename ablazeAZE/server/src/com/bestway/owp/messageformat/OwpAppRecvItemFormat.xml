<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="eportmsg.xsl"?>
<!--Element message: 此元素内所有内容及属性描述一个报文的定义及从数据库的获取途径。-->
<message version="1.0">
	<section multiple="false" required="true">
		<description>外发加工申请表表体收回商品明细</description>
		<section-flag>FPT2_APP_OUT_ITEM</section-flag>
		<data-id>OwpAppRecvItem</data-id>
		<field nullable="false" signNo="1">
			<description>外发序号</description>
			<field-name>seqNum</field-name>
			<format>Z(8)9</format>
			<field-flag>LIST_NO</field-flag>
		</field>
		<field nullable="true" signNo="2">
			<description>商品编码</description>
			<field-name>complex.code</field-name>
			<format>X(10)</format>
			<field-flag>CODE_T_S</field-flag>
		</field>
		<field nullable="false" signNo="3">
			<description>商品名称</description>
			<field-name>hsName</field-name>
			<format>X(50)</format>
			<field-flag>G_NAME</field-flag>
		</field>
		<field nullable="false" signNo="4">
			<description>规格型号</description>
			<field-name>hsSpec</field-name>
			<format>X(50)</format>
			<field-flag>G_MODEL</field-flag>
		</field>
		<field nullable="false" signNo="5">
			<description>计量单位</description>
			<field-name>hsUnit.name</field-name>
			<format>X(3)</format>
			<field-flag>UNIT</field-flag>
		</field>

		<field nullable="false" signNo="6">
			<description>申报数量</description>
			<field-name>qty</field-name>
			<format>Z(12)9.9(5)</format>
			<field-flag>QTY</field-flag>
		</field>
		<field nullable="true" signNo="12">
			<description>备注</description>
			<field-name>note</field-name>
			<format>X(50)</format>
			<field-flag>NOTE</field-flag>
		</field>
		<field nullable="false" signNo="14">
			<description>修改标志</description>
			<field-name>modifyMark</field-name>
			<format>X(1)</format>
			<field-flag>MODIFY_MARK</field-flag>
		</field>
	</section>
</message>
