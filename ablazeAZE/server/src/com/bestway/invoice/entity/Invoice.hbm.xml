<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.invoice.entity.Invoice" table="invoice">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="versionCode"/>
		<property name="invoiceCode"/>
		<property name="customsDeclaredCode"/>
		<property name="money"/>
		<property name="beginDate"/>
		<property name="endDate"/>
		<property name="state"/>
		<property name="memo"/>
		<property name="projectType"/>
		<property name="draftDate"/>
		<property name="usedDate"/>
		<property name="canceledDate"/>
		<property name="cavDate"/>
		<property name="customsDeclarationId"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="invoice_company"/>
	</class>
</hibernate-mapping>
