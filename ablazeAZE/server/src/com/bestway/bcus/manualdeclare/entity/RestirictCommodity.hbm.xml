<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" 
		name="com.bestway.bcus.manualdeclare.entity.RestirictCommodity" 
		table="restirictCommodity">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="types"/>
		<property name="seqNum"/>
		<property name="name"/>
		<property name="spec"/>
		<property name="complex" />
		<property name="unit"/>
		<property name="amount"/>
		<property name="vindicadate"/>
		<property name="vindicator"/>
		<property name="revertdate"/>
		<property name="revertuser"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" 
			index="restirictCommodity_company"/>
	</class>
	
</hibernate-mapping>
