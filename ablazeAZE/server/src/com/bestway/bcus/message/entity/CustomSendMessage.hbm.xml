<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.message.entity.CustomSendMessage" table="customSendMessage">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="tcsTaskId" />
		<property name="projectType" />
		<property name="seqNo" />
		<property name="sendDate" />
		<property name="fileName"/>
		<property name="emsNo" />
		<property name="sendUser" />
		<property name="bgdcheckResult" />								
		<property name="ylrh" />
		<property name="success" />
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="messagequery_company"/>
	</class>

</hibernate-mapping>
