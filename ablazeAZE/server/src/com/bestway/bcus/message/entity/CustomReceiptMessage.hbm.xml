		<property name="fileName" />								
		<property name="uniteCode" />
		<property name="customs" />
		<property name="ylrh" />
		<property name="success" />
		<property name="noticeDate" />
		<property name="eportLocationCode" />
		<property name="corporationName" />
		<property name="entrydeclarantNo" />
		<property name="corporationCustomsCode"/>
		<property name="cyNo" />
		<property name="warehouseNo" />
		<property name="importExportDate" />
		<property name="packages" />								
		<property name="billOfLadingNo" />
		<property name="meansOfTransportMode" />
		<property name="meansOfTransportId" />
		<property name="netWeight" />
		<property name="grossWeight" />
		<property name="declareDate" />
