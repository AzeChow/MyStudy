			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
		<many-to-one lazy="false"      name="district" column="district" 
			class="com.bestway.bcus.custombase.entity.countrycode.District"/>	
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="customsdset_company"/>
		<many-to-one lazy="false"      name="brokerCorp" column="brokerCorp" 
			class="com.bestway.pis.entity.BrokerCorp"/>	
    </class>

</hibernate-mapping>
