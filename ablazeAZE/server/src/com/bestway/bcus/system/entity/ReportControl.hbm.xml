<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.bcus.system.entity.ReportControl" table="ReportControl">
       	<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="secondLegalUnit"/>
       	<property name="secondAmount"/>
       	<property name="uses"/>
       	<property name="levyMode"/>
       	<property name="pieces"/>
       	<property name="wrapType"/>
       	<property name="wrapTypeDetail"/>
       	<property name="customs"/>
       	<property name="impExpDate"/>
       	<property name="transferMode"/>
       	<property name="levyKind"/>
       	<property name="license"/>
       	<property name="countryOfLoadingOrUnloading"/>
       	<property name="domesticDestinationOrSource"/>
       	<property name="portOfLoadingOrUnloading"/>
       	<property name="authorizeFile"/>
       	<property name="transac"/>
       	<property name="grossWeight"/>
       	<property name="netWeight"/>
       	<property name="commodityNum"/>
       	<property name="containerNum"/>
       	<property name="attachedBill"/>
       	<property name="declarationCustoms"/>
       	<property name="certificateCode"/>
       	<property name="memos"/>
       	<property name="predock"/>
       	<property name="bondedWarehouse"/>
       	<property name="relativeManualNo"/>
       	<property name="relativeId"/>
       	<property name="customsEnvelopBillNo"/>
       	<property name="invoiceCode"/>
       	<property name="preCustomsDeclarationCode"/>
       	<property name="CustomdeclarationMun"/>
       	<property name="creater"/>
       	<property name="contractAmount"/>
       	<property name="detailNote"/>
       	<property name="customerCode"/>
       	<many-to-one lazy="false" name="company" column="coid"
			class="com.bestway.bcus.system.entity.Company" />
    </class>

</hibernate-mapping>
