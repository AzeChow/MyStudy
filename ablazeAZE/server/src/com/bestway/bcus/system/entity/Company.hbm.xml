		<property name="tel" />
		<property name="amountIn" />
		<property name="amountOut" />
		<property name="amountProduct" />
		<property name="bank" />
		<property name="account"/>
		<many-to-one lazy="false" name="masterCustoms" column="masterCustoms" 
			class="com.bestway.bcus.custombase.entity.countrycode.Customs"/>
		<property name="inArea" />
		<property name="outArea" />
		<property name="sysName" />
		<property name="sysOwner" />
		<property name="outTradeCo"/>
		<property name="outCoTel" />
		<property name="outAddress" />
		<property name="outLinkMan" />
		<property name="outLinkManager" />
		<property name="outFax" />
		<property name="sysFunc" />
		<property name="consent"/>
		<property name="counFlag" />
		<property name="ownercounFlag" />
		<property name="description"/>
		<property name="appCusMan" />
		<property name="appCusManTel" />
		<property name="linkman"/>
		<property name="englishName"/>
		<property name="englishAddr"/>
		<property name="isCurrCompany"/>
		<property name="bcsFtpAddress"/>
		<property name="bcsFtpUser"/>
		<property name="bcsFtpPassword"/>
		<property name="bcsEDICode"/>
		<property name="dzscFtpAddress"/>
		<property name="dzscFtpUser"/>
		<property name="dzscFtpPassword"/>
		<property name="dzscEDICode"/>
		<property name="bcusftpAddress"/>
		<property name="bcusftpUser"/>
		<property name="bcusftpPassword"/>
		<property name="bcusbcusEDICode"/>
