<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.bcus.system.entity.CommonCheckNull" table="commonchecknull">
		<cache usage="read-write"/> 
       	<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="className"/>
		<property name="classMemo"/>
		<property name="fieldName"/>
		<property name="fieldMemo"/>
		<property name="fieldType"/>
		
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company"/>
	   
    </class>

</hibernate-mapping>
