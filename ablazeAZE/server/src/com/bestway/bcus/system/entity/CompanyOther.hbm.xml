		<property name="isSendMail" />
		<property name="sendMail" />
		<property name="isAllowBGDDetailExceed20" />
		<property name="isCheckWeight" />
		<property name="transportTool" />
		<property name="sendCustoms"/>
		<property name="invoiceAddress"/>
		<property name="isSaveCustomsEnvelopBillNo"/>
		<property name="isFilter"/>
		<property name="isSumMoney"/>
		<property name="isSpecification"/>
		<property name="isExportPackinglistOrInvoice"/>
		<property name="isTransitadd"/>
