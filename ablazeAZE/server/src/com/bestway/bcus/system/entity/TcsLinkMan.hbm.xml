<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.bcus.system.entity.TcsLinkMan" table="tcslinkman">
       	<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="name" length="70"/>
       	<property name="department" length="70"/>
       	<property name="duty" length="70"/>
       	<property name="telephone" length="35"/>		
       	<property name="mobile" length="35"/>
       	<property name="fax" length="35"/>
       	<property name="address" length="255"/>
		<property name="zipCode" length="70"/>
		<property name="email" length="70"/>
		<property name="imCode" length="70"/>
		<property name="imType" length="3"/>
    </class>

</hibernate-mapping>
