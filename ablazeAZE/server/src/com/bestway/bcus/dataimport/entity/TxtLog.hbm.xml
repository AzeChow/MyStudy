<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.TxtLog" table="txtlog">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="type" length="100"/>
		<property name="taskName" length="225"/>
		<property name="result" length="225"/>
		<property name="otherNote" length="225"/>
		<property name="beginDate" length="100"/>
		<property name="endDate" length="100"/>
		<property name="computer" length="225"/>
		<property name="excuter" length="225"/>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="txtlog_company"/>
	</class>

</hibernate-mapping>