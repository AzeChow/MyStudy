<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.DBFormatSetup" 
		table="dbformatsetup">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="dbFormat" column="dbFormat"
			 class="com.bestway.bcus.dataimport.entity.DBFormat"/>
		<many-to-one lazy="false"      name="objName" column="objName"
			 class="com.bestway.bcus.dataimport.entity.ClassList"/>	
		<many-to-one lazy="false"      name="gbStr" column="gbStr"
			 class="com.bestway.bcus.dataimport.entity.FieldList"/>	
		<many-to-one lazy="false"      name="gbStrChild" column="gbStrChild"
			 class="com.bestway.bcus.dataimport.entity.FieldList"/>		 
	    <property name="aimName" length="225"/>
		<property name="aimFieldName" length="225"/>
		<property name="aimFieldType" length="225"/>
		<property name="aimFieldLen" />
		<property name="iskey" />
		<property name="sourceFieldName" length="225"/>
		<property name="sourceFieldType" length="225"/>	
		<property name="sourceFieldLen" />
		<property name="orderNo"/>
		<property name="isMatch" />
		<property name="creator" length="225"/>	
		<property name="createdate" />
		<property name="gbflag" length="225"/>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="dbformatsetup_company"/>
	</class>
</hibernate-mapping>