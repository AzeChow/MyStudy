<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.DBTaskSel" 
		table="dbtasksel">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="seqNum"/>
		<many-to-one lazy="false"      name="dbtaskEx" column="dbtaskEx"
			 class="com.bestway.bcus.dataimport.entity.DBTaskEx"/>
			 
	    <many-to-one lazy="false"      name="dbFormat" column="dbFormat"
			 class="com.bestway.bcus.dataimport.entity.DBFormat" />
			 
	    <many-to-one lazy="false"      name="execuSql" column="execuSql"
			 class="com.bestway.bcus.dataimport.entity.DBDataExecuSql" />

		<many-to-one lazy="false"      name="subDBTaskEx" column="subdbtaskEx"
			 class="com.bestway.bcus.dataimport.entity.DBTaskEx"/>
			 
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="dbtasksel_company"/>
	</class>

</hibernate-mapping>