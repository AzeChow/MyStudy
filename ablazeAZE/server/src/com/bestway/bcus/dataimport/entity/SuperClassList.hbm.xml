<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.SuperClassList" 
		table="superclasslist">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="seqNum" />
		<property name="taskName" length="225"/>
		<property name="memo" length="255"/>
		<property name="importSelect" />
		<property name="isFtoJ" />
		<property name="className"/>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="superclasslist_company"/>
	</class>
</hibernate-mapping>