<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.TxtTaskSel" 
		table="txttasksel">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="seqNum"/>
		
		<many-to-one lazy="false"      name="txttask" column="taskid"
			 class="com.bestway.bcus.dataimport.entity.TxtTaskEx"/>
		<many-to-one lazy="false"      name="execuSql" column="execuSql"
			 class="com.bestway.bcus.dataimport.entity.DBDataExecuSql" />
			 	 
	   <many-to-one lazy="false"      name="txttable" column="tableid"
			 class="com.bestway.bcus.dataimport.entity.TxtTask" />
<!--	TableId-->
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="txttasksel_company"/>
		<many-to-one lazy="false"      name="subDBTaskEx" column="subDBTaskEx" 
			class="com.bestway.bcus.dataimport.entity.TxtTaskEx"/>	
			
	</class>

</hibernate-mapping>