<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.FieldList" 
		table="fieldlist">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="classList" column="classList"
			 class="com.bestway.bcus.dataimport.entity.ClassList"/>
		<many-to-one lazy="false"      name="objName" column="objName"
			 class="com.bestway.bcus.dataimport.entity.ClassList"/>	 
	    <property name="sortno" />
		<property name="fieldname" length="225"/>
	    <property name="name" length="225"/>
		<property name="fieldlen" />
		<property name="fieldtype" length="225"/>
		<property name="fielddesc" length="255"/>
		<property name="iskey" />
		<property name="isNull"/>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="fieldlist_company"/>
	</class>
</hibernate-mapping>