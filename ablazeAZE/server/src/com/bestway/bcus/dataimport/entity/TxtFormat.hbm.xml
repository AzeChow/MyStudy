<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.TxtFormat" 
		table="txtformat">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="txttask" column="taskid"
			 class="com.bestway.bcus.dataimport.entity.TxtTask"/>
		<many-to-one lazy="false"      name="objName" column="objName"
			 class="com.bestway.bcus.dataimport.entity.ClassList"/>	
		<many-to-one lazy="false"      name="gbStr" column="gbStr"
			 class="com.bestway.bcus.dataimport.entity.FieldList"/>	
	    <property name="sortno" />
		<property name="fieldname" />
		<property name="name" length="225"/>
		<property name="fieldlen" />
		<property name="fieldtype" length="20"/>
		<property name="fielddesc" length="200"/>	
		<property name="creator" length="20"/>
		<property name="createdate" />		
		<property name="editor" length="20"/>	
		<property name="iskey" />
		<property name="gbflag" />
		<property name="isNull"/>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="txtformat_company"/>
	</class>
</hibernate-mapping>