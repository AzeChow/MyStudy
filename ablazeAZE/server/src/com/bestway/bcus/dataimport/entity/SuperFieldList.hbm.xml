<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.dataimport.entity.SuperFieldList" 
		table="superfieldlist">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="seqNum" />
		<property name="fieldname"/>
		<property name="name"/>
		<property name="fieldtype"/>
		<property name="isNullValue"/>
		<property name="childClassName"/>
		<property name="childFieldName"/>
		<property name="isOnlyValue"/>
		<many-to-one lazy="false"      name="superClassList" column="superClassList" 
			class="com.bestway.bcus.dataimport.entity.SuperClassList"/>
			
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="superfieldlist_company"/>
			
	</class>
</hibernate-mapping>