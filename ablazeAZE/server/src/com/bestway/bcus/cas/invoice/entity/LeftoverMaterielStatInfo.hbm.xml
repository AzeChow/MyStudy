<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.invoice.entity.LeftoverMaterielStatInfo" table="LMStatInfo">
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>	
	    <property name="complex" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnitName" />
		
		<property name="f1"/>
		<property name="f2"/>
		<property name="f3"/>
		<property name="f4"/>
		<property name="f5"/>
		<property name="f6"/>
		<property name="f7"/>
		
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="exgexpCustom_company"/>
	</class>
</hibernate-mapping>
		