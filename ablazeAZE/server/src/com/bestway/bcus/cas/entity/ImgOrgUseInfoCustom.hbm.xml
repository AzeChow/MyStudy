		<property name="f12"/>
		<property name="f13"/>
		<property name="f14"/>
		<property name="f15"/>
		<property name="f16"/>
		<property name="f17"/>
		<property name="f18"/>
		<property name="f19"/>
		<property name="f20"/>
		<property name="f21"/>
<!--		<property name="f22"/>
		<property name="f23"/>-->
		<property name="f24"/>
		<property name="f25"/>
		<property name="f26"/>
		<property name="f27" length="255"/>
		<property name="money"/>
