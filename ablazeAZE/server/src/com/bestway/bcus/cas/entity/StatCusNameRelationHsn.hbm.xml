<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" 
		name="com.bestway.bcus.cas.entity.StatCusNameRelationHsn" 
		table="statcusnamerelationhsn">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<!--<many-to-one lazy="false" name="statCusNameRelation" 
			column="statcusnamerelation" 
			class="com.bestway.bcus.cas.entity.StatCusNameRelation"/>-->
		<property name="seqNum"/>
		<property name="cusName"/>
		<property name="cusSpec"/>
		<property name="unitConvert"/>
		<property name="emsNo"/>
		<property name="emsBeginDate"/>
		<property name="emsEndDate"/>
		<property name="projectType"/>
		<property name="materielType"/>
		<many-to-one lazy="false" name="complex" column="complex" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex" 
			index="sttuselation_complex"/>
		<many-to-one lazy="false" name="cusUnit" column="unitCode" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" 
			index="sttuselation_company"/>
		<property name="version"/>
	</class>
</hibernate-mapping>
