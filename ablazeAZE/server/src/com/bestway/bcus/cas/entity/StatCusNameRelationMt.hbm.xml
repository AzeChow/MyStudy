<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.entity.StatCusNameRelationMt" 
		table="statcusnamerelationmt">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false" name="statCusNameRelation" column="statcusnamerelation" 
			class="com.bestway.bcus.cas.entity.StatCusNameRelation"/>
		<many-to-one lazy="false" name="materiel" column="materiel" 
			class="com.bestway.common.materialbase.entity.Materiel" index="stausereltionmt_Materiel"/>			
		<property name="unitConvert"/>
		<property name="ptNo"/>
		<property name="factoryName"/>
		<property name="factorySpec"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="stausereltionmt_company"/>
	</class>
</hibernate-mapping>
