<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.entity.CheckDetail" table="checkdetail">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="checkMaster" column="checkMaster" class="com.bestway.bcus.cas.entity.CheckMaster"/>
		<property name="ptPart" length="100"/>
		<property name="ptName" length="100"/>
		<property name="ptSpec" length="100"/>
		<property name="ptUnit" length="100"/>
		<property name="ptAmount"/>
		<property name="factPtAmount"/>
		<property name="removeAmount"/>
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="checkdetail_company"/>
	</class>
</hibernate-mapping>
