<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.entity.LeftoverMaterielBalanceInfo" table="LMBalanceInfo">
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>	
	    <property name="complex" />
		<property name="name" />
		<property name="spec" />
		<property name="unitName" />
		
		<property name="f1"/>
		<property name="f2"/>
		<property name="f4"/>
		<property name="f5"/>
		
		<property name="unitPrice"/>
		<property name="outRate"/>
		<property name="taxRate"/>		
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="exgexpCustom_company"/>
	</class>
</hibernate-mapping>
		