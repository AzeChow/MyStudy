<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcus.cas.entity.TempObjectId" 
		table="TempObjectId">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="objId" index="debtdetail_objIdssf"/>
		<property name="uuid" index="debtdetail_uuidsssf"/>
	</class>
</hibernate-mapping>