<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.entity.CheckBalanceOfCustom" table="CheckBalanceOfCustom">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="checkDate"/>
		<property name="name"/>
		<property name="spec"/>
		<property name="unitName"/>
		<property name="checkAmount"/>
		<property name="note"/>		
		<property name="kcType"/>
		<many-to-one lazy="false" name="wareSet" column="wareSet" 
			class="com.bestway.common.materialbase.entity.WareSet"/>	
		<many-to-one lazy="false" name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="CheckBalance_company"/>
	</class>
</hibernate-mapping>
