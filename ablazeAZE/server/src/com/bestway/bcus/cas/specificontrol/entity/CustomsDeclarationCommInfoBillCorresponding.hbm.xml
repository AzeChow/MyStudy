<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class 
		name="com.bestway.bcus.cas.specificontrol.entity.CustomsDeclarationCommInfoBillCorresponding" 
		table="CusBillCorresponding">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="alreadyCorrespondingAmount"/>
		<property name="unitConvert"/>
		<property name="customsDeclarationId"/>
		<property name="customsDeclarationCommInfoId"/>
		<property name="customsDeclarationCode"/>
		<property name="emsHeadH2k"/>
		<property name="impExpType"/>
		<property name="serialNumber"/>
		<property name="commSerialNo"/>
		<property name="commName"/>
		<property name="commSpec"/>
		<property name="commAmount"/>
		<property name="commUnitPrice"/>		
		<property name="impExpDate"/>
		<many-to-one lazy="false" name="unit" column="unit" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false" name="legalUnit" column="legalUnit" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false" name="secondLegalUnit" column="secondLegalUnit" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<property name="firstAmount"/>
		<property name="secondAmount"/>
		<property name="version"/>
		
		<property name="envelopNo"/>
		
		<many-to-one lazy="false" name="scmCoc" column="scmCoc" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>
		<many-to-one lazy="false" name="complex" column="complex" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex" index="CusBillCorepnd_complex"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="CusBillCorepnd_company"/>
	</class>
</hibernate-mapping>
		