<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.parameterset.entity.BillCorrespondingOption" 
		table="billCorrespondingOption">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>	
		<property name="isCalculatePrice"/>
		<property name="isOverYear"/>
		<property name="isBillCorrespondingAffirm"/>	
		<property name="isNoNameMatching"/>
		<property name="isNamePartMatching"/>
		<property name="isNoSpecMatching"/>	
		<property name="isNoUnitMatching"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index=""/>
	</class>
</hibernate-mapping>
		