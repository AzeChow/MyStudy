<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.bill.entity.BillDetailMateriel" 
		table="BillDetailMateriel">
		<!--<cache usage="read-write"/>-->
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false" name="wareSet" column="wareSet" 
			class="com.bestway.common.materialbase.entity.WareSet"/>
		<many-to-one lazy="false" name="billMaster" column="billMasterId" 
			class="com.bestway.bcus.cas.bill.entity.BillMasterMateriel" index="BillDetailMateriel_billMaster"/>
		<property name="ptName" length="255"/>
		<property name="ptSpec" length="255"/>
		
		
		<many-to-one lazy="false" name="ptUnit" column="ptUnitId" 
			class="com.bestway.common.materialbase.entity.CalUnit"/>
		<property name="ptPart" length="255"/>
		<property name="ptAmount"/>
		<property name="ptPrice"/>
		<!--<many-to-one lazy="false" name="curr" column="currId" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>-->
		<property name="productNo" length="255"/>
		<many-to-one lazy="false" name="complex" column="complexid" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex" index="BillDetailMateriel_comliex"/>
		<property name="hsName" length="255"/>
		<property name="hsSpec" length="255"/>
		<property name="version"/>
		<property name="hsPrice"/>
		<property name="money"/>
		<property name="seqNum"/>
		<property name="hsAmount"/>
		<many-to-one lazy="false" name="hsUnit" column="hsUnitId" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<property name="netWeight"/>
		<property name="customNo" length="255"/>
		<property name="customNum"/>
		<property name="note"/>
		<property name="isTransferFactoryBill" column="isTransferCER"/>
		<property name="isImpExpRequestBill"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="BillDetailMateriel_company"/>
			<property name="billNo"/>
		<property name="tempDetailNo"/>
		<property name="tempDetailNo1"/>
		<property name="tempDetailNo2"/>
		<property name="tempDetailNo3"/>
		<property name="emsNo"/>
		<property name="hsVersion"/>		
		<property name="invoiceNo"/>		
		<property name="takeBillNo"/>		
		<property name="oderBillNo"/>	
		<property name="unitConvert"/>	
	</class>
</hibernate-mapping>
		