<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.cas.bill.entity.BillMasterLeftoverMateriel" 
		table="BMLMateriel">
		<!--<cache usage="read-write"/>-->
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		
		<many-to-one lazy="false" name="billType" column="billTypeId" 
			class="com.bestway.bcus.cas.entity.BillType"/>		
			<property name="billNo" />	
					<property name="isValid"/>
		<property name="validDate"/>
		<property name="keepAccounts"/>
		<property name="isFlag"/>
		<property name="tempNo"/>
		<many-to-one lazy="false" name="scmCoc" column="scmCoc" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="BilMastertoverMateri_company"/>		
		<property name="tempNo1"/>
		<property name="tempNo2"/>
		<property name="tempNo3"/>	
		<property name="envelopNo"/>	
		<property name="notice"/>
	</class>
</hibernate-mapping>
