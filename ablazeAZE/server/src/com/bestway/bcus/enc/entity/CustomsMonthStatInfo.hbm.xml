<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.enc.entity.CustomsMonthStatInfo" 
		table="customsmonthstatinfo">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="emsNo" />
		<property name="statTerm" />
		<property name="seqNum" />
		<many-to-one lazy="false"      name="complex" column="complex" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex"/>
		<property name="name" />
		<property name="spec" />
		<many-to-one lazy="false"      name="unit" column="unit" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<property name="preAmount" />
		<property name="preMoney" />
		<property name="currentImpAmount" />
		<property name="currentUsedAmount" />
		<property name="currentAmount" />
		<property name="currentImpMoney" />
		<property name="endAmount" />
		<property name="endMoney" />
		<property name="averageUnitPrice" />
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="monthstatinfo_company"/>
	</class>
</hibernate-mapping>
