<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.enc.entity.ExpBackPortRequestBook" 
		table="expbackportrequestbook">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"   name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="expbaortreestb_company"/>
		<many-to-one lazy="false"   name="impExpRequestBill" 
			class="com.bestway.bcus.enc.entity.ImpExpRequestBill" column="impExpRequestBillId"/>		
		<property name="hongKongCommodityNo"/>
		<property name="contractNo" />
		<property name="conveyance"/>
		<property name="requestDate"/>
		<property name="requestExcuse" />	
	</class>
</hibernate-mapping>