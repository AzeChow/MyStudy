<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.enc.entity.CustomsFromMateriel" 
		table="customsfrommateriel">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"  name="materiel" column="materielid" 
			class="com.bestway.common.materialbase.entity.Materiel" index="cstsfrmmaiel_Materiel"/>	
		
		<property name="infoid"/>
		<property name="ptNum"/>
		<property name="sumBoxNum"/>
		<property name="boxNum"/>
		<property name="sumNetWeight"/>
		<property name="sumGrossWeight"/>
		<property name="sjNetWeight"/>
		<property name="sjGrossWeight"/>
		
		<property name="bgCommAmount"/>
		<property name="bgCommTotalPrice"/>
		<property name="bgNetWeight"/>
		<property name="bgGrossWeight"/>
		<property name="bgFirstAmount"/>
		<property name="bgSecondAmount"/>
		<property name="bgPieces"/>
		
		<many-to-one lazy="false"  name="scmCoc" column="scmCoc" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>	
			
		<many-to-one lazy="false"  name="bcusimgbill" column="bcusimgbill" 
			class="com.bestway.bcus.manualdeclare.entity.EmsHeadH2kImg"/>		
        
		<many-to-one lazy="false"  name="bcusexgbill" column="bcusexgbill" 
			class="com.bestway.bcus.manualdeclare.entity.EmsHeadH2kExg"/>	
			<property name="bcusimgNo"/>
			<property name="bcusexgNo"/>

		<many-to-one lazy="false"  name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="cstsfrmmaiel_company"/>

		
	</class>
</hibernate-mapping>
