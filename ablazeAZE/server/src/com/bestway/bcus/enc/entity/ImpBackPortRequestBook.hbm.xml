<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.enc.entity.ImpBackPortRequestBook" 
		table="impbackportrequestbook">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"  name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="impbcporequestbk_company"/>
		<many-to-one lazy="false"  name="impExpRequestBill" 
			class="com.bestway.bcus.enc.entity.ImpExpRequestBill" 
			column="impExpRequestBillId"/>
		<property name="contractFlag"/>
		<property name="contractNo" />
		<property name="year" />
		<property name="month" />
		<property name="day" />
		<property name="backFactoryExcuse"/>
		<property name="productName"/>
	</class>
</hibernate-mapping>