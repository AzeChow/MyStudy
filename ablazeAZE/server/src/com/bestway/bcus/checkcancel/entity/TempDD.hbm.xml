<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.checkcancel.entity.TempDD" table="tempdd">
		<cache usage="read-write"/> 
		<id name="cpseqnum1" column="cpseqnum1" length="50">
			<generator class="assigned"/>			
	    </id>
		<property name="cpseqnum"/>
		<property name="version"/>
		<property name="ljseqnum" />
		<property name="unitUse" />
		<property name="wear" />
		<property name="aa" />
		<property name="bb" />
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" 
			index="tempdd_codmpany"/>
	</class>

</hibernate-mapping>
