<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.checkcancel.entity.EmsTransFactory" table="EmsTransFactory">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="head" column="head" class="com.bestway.bcus.checkcancel.entity.EmsAnalyHead"/>
		<property name = "ptNo"/>
		<property name = "ptName"/>
		<property name = "ptSpec"/>
		<property name = "unTransferNum"/>
		<property name = "unReceiveNum"/>
		<many-to-one lazy="false"      name="calUnit" column="calUnit" class="com.bestway.common.materialbase.entity.CalUnit"/>
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="emspdimg_company"/>
	</class>

</hibernate-mapping>
