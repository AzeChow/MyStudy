<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.checkcancel.entity.FactoryCheckImg" table="factorycheckimg">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="head" column="head" class="com.bestway.bcus.checkcancel.entity.CheckParameter"/>
		<property name="ptNo"/>
		<property name="name"/>
		<property name="spec"/>
		<property name = "materStockNum"/>
		<property name="bgNum"/>
		<many-to-one lazy="false"      name="calUnit" column="calUnit" class="com.bestway.common.materialbase.entity.CalUnit"/>
		<many-to-one lazy="false"      name="unit" column="unit" class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="factorycheckimg_company"/>
	</class>

</hibernate-mapping>
