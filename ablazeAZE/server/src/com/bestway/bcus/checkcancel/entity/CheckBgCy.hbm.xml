<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.checkcancel.entity.CheckBgCy" table="checkbgcy">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false"      name="head" column="head" class="com.bestway.bcus.checkcancel.entity.CheckParameter"/>
		<property name = "seqNum"/>
		<property name = "name"/>
		<property name = "spec"/>
		<property name = "emsBalance"/>
		<property name = "factNum"/>
		<property name = "cyNum"/>
		<many-to-one lazy="false"      name="unit" column="unit" class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="checkbgcy_company"/>
	</class>

</hibernate-mapping>
