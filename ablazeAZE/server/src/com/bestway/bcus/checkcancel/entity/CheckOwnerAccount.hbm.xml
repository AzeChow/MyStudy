<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.checkcancel.entity.CheckOwnerAccount" table="checkowneraccount">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
        <property name = "ptNo"/>
		<property name="name"/>
		<property name="spec"/>
		<property name="materielStockNum"/>
		<property name="materielOnwayNum"/>
		<property name="exgConvertImgNum"/>
		<property name="tranNoCustomsNum"/>
		
		<property name="onLineNum"/>
		<property name="halfExgConvertImgNum"/>
		<property name="flotsamNum"/>
		<many-to-one lazy="false"      name="unit" column="unit" class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
        <many-to-one lazy="false"      name="head" column="head" class="com.bestway.bcus.checkcancel.entity.CheckParameter"/>	
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="checkOwenrhead_company"/>
	</class>

</hibernate-mapping>
