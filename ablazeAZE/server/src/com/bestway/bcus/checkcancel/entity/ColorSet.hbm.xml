<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcus.checkcancel.entity.ColorSet" table="colorset">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="beginNum"/>
		<property name="endNum"/>
		<property name="color" />
		
		<many-to-one lazy="false"      name="company" column="coid" class="com.bestway.bcus.system.entity.Company" index="colorset_company"/>
	</class>

</hibernate-mapping>
<!-- parsed in 0ms -->

		