<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcus.checkstock.entity.ECSStockAnalyse"
		table="ecsstockanalyse">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="serialNumber" />
		<property name="emsNo" />
		<property name="seqNum" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnit" />
		<property name="stockAmountImg" not-null="true"/>
		<property name="stockAmountExg" not-null="true"/>
		<property name="stockAmountBuyImg" not-null="true"/>
		<property name="stockAmountOutFactoryImg" not-null="true"/>
		<property name="stockAmountOutSendExg" not-null="true"/>
		<property name="stockAmountProcessImg" not-null="true"/>
		<property name="stockAmountTravelingExg" not-null="true"/>
		<property name="stockAmountTravelingImg" not-null="true"/>
		<property name="stockAmountBadImg" not-null="true"/>
		<property name="stockAmountSemiExgHStore" not-null="true"/>
		<property name="stockAmountFinishingExg" not-null="true"/>
		<property name="stockAmountOutSendExgPt" not-null="true"/>
		<property name="stockTotalAmount" not-null="true"/>
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcus.checkstock.entity.ECSSection" index="ecsstockanalyse_section"/>
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="ecsstockanalyse_company" />
	</class>
</hibernate-mapping>
