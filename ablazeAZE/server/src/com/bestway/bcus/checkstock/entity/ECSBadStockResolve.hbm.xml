<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcus.checkstock.entity.ECSBadStockResolve"
		table="ecsbadstockresolve">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="serialNumber" />
		<property name="emsNo" />
		<property name="seqNum" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnit" />
		<property name="imgHsAmount"/>
		<property name="finishedHsAmount" />
		<property name="semiFinishedHsAmount" />
		<property name="hsAmount" />
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcus.checkstock.entity.ECSSection" index="ecsbadstockresolve_section"/>
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="ecsbadstockresolve_company" />
	</class>
</hibernate-mapping>
	<!-- parsed in 0ms -->