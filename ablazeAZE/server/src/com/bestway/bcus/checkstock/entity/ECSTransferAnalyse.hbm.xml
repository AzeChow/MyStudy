<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcus.checkstock.entity.ECSTransferAnalyse"
		table="ecstransferanalyse">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="serialNumber" />
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcus.checkstock.entity.ECSSection" index="ecstransferdiffcount_section"/>
		<property name="emsNo" />
		<property name="seqNum" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnit" />
		<property name="unTransHadSendNum" not-null="true"/>
		<property name="unSendHadTransNum" not-null="true"/>
		<property name="unTransHadReceiveNum" not-null="true"/>
		<property name="unReceiveHadTransNum" not-null="true"/>
		<property name="diffAmount" not-null="true" />
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="ecstransferdiffcount_company" />
	</class>
</hibernate-mapping>
