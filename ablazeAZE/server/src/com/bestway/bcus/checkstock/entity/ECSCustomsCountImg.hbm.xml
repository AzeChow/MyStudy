<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcus.checkstock.entity.ECSCustomsCountImg"
		table="ecscustomscountimg">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="serialNumber" />
		<property name="emsNo" />
		<property name="seqNum" />
		<property name="commName" />
		<property name="commSpec" />
		<property name="unit" />
		<property name="qcAmount" not-null="true" />
		<property name="directImportAmount" not-null="true" />
		<property name="transferExportAmount" not-null="true" />
		<property name="backMaterExportAmount" not-null="true" />
		<property name="remainForwardImportAmount" not-null="true" />
		<property name="remainForwardExportAmount" not-null="true" />
		<property name="totalImportAmount" not-null="true" />
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcus.checkstock.entity.ECSSection" index="ecscustomscountimg_section"/>
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="ecscustomscountimg_company" />
	</class>
</hibernate-mapping>
