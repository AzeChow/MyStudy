<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fecav.entity.StrikeBillOfExchange" table="strikebillofexchange">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<many-to-one lazy="false" name="fecavBillStrike" class="com.bestway.fecav.entity.FecavBillStrike"/>	
		<property name="billOfExchangeId"/>		
		<property name="billOfExchangeCode"/>
		<property name="endDate"/>
		<property name="exchangeMoney"/>
		<property name="exchangeRate"/>		
		<property name="strikeMoney"/>
		<property name="remainMoney"/>
		<property name="converUSDMoney"/>
		<many-to-one lazy="false"  name="curr" column="currency" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
		
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="strikebillofexchange_1"/>	
		</many-to-one>
    </class>

</hibernate-mapping>
