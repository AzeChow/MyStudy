<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fecav.entity.FecavBillStrike" table="fecavbillstrike">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="signInNo"/>
		<property name="cavSignInMan"/>
		<property name="cavSignInDate"/>
		
		<property name="financeSignInMan"/>
		<property name="financeSignInDate"/>
		
		<property name="strikeMan"/>
		<property name="strikeDate"/>		
		
		<property name="cancelMan"/>
		<property name="cancelDate"/>
		
		<property name="closeAccountMan"/>
		<property name="closeAccountDate"/>		
	
		<property name="canStrikeTotalMoney"/>		
		<property name="canStrikeImportMoney"/>
		<property name="canStrikeExchangeMoney"/>
		
		<property name="strikedImportMoney"/>
		<property name="strikedExchangeMoney"/>
		
		<property name="fecavState"/>
		
		<property name="pieces"/>
		<property name="projectType"/>
		<property name="emsNo"/>
		<property name="strikeRate"/>
		
		<many-to-one lazy="false"  name="curr" column="currency" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
			
		<many-to-one lazy="false"      name="tradeMode" column="tradeMode" 
			class="com.bestway.bcus.custombase.entity.parametercode.Trade"/>	
						
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="fecavbillstrike_1"/>	
		</many-to-one>
    </class>

</hibernate-mapping>
