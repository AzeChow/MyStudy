<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.common.dataexport.entity.TxtToDBRegionSetup" 
		table="TxtToDBRegionSetup">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="destFieldName"/>
		<property name="destFieldType"/>
		<property name="destFieldTypeDesc" column="destTypeName"/>
		<property name="srcFieldName"/>
		<!--<property name="srcFieldType"/>
		<property name="srcFieldTypeDesc" column="srcTypeName"/>-->	
	
		<property name="sqlStr1" column ="sqlScript" length="225"/>
		<property name="sqlStr2" length="225"/>
		<property name="sqlStr3" length="225"/>
		<property name="sqlStr4" length="225"/>
		<property name="sqlStr5" length="225"/>
		<property name="isCache"/>
		<property name="isKey"/>
		<property name="isBig5DataBase"/>		
		<property name="isPrimaryKey"/>			
		<property name="jdbcFlag"/>		
			
		<many-to-one lazy="false"      name="paraJDBCDataSource" column="paraJDBCDataSource"
			 class="com.bestway.common.dataexport.entity.JDBCDataSource"/>
			 	
		<many-to-one lazy="false" name="txtToDBRegion" column="txtToDBRegion" 
			class="com.bestway.common.dataexport.entity.TxtToDBRegion"/>
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" />
	</class>
</hibernate-mapping>