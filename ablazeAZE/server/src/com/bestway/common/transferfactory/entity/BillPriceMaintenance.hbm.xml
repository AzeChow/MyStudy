<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class 
		name="com.bestway.common.transferfactory.entity.BillPriceMaintenance" 
		table="billpricemaintenance">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="seqNum" />
		<many-to-one lazy="false" name="complex" column="complex" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex"/>
		<property name="ptName" />
		<property name="projectType" />
		<property name="ptSpec" />
		<property name="isCustomer" />
		<many-to-one lazy="false" name="scmCoc" column="scmCocId" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>
		<many-to-one lazy="false" name="curr" column="currid" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
		<property name="unitPrice" />
		<many-to-one lazy="false" name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" 
			index="billPriceMaintenance_company"/>
	</class>
</hibernate-mapping>