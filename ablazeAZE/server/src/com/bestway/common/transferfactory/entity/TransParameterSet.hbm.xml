<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class 
		name="com.bestway.common.transferfactory.entity.TransParameterSet" 
		table="transparameterset">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="customsEnvelopUsedMultiple" />		
		<property name="isAutoJieAn" />	
		<property name="isNoEdit" />
		<property name="isLimit" />
		<many-to-one lazy="false" name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" 
			index="transparameterset_company"/>
	</class>
</hibernate-mapping>