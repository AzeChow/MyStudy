<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.common.transferfactory.entity.CustomsEnvelopRequestBill" table="customsenveloprequestbill">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="billNo"/>
		<property name="isAvailability" />
		<property name="isMakeCustomsEnvelop" />
		<property name="isImpExpGoods" />
		<property name="buyerSalesman"/>
		<property name="applyToCustomserEmployee"/>
		<property column="cancelVerficationEmployee" name="cancelAfterVerificationEmployee"/>
		<property name="lister" />
		<property name="deliverGoodsAddress" />		
		<property name="memo" length="200"/>
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="customneopqustbill_company"/>
		<many-to-one lazy="false"      name="scmCoc" column="scmCocId" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>		
	</class>
</hibernate-mapping>