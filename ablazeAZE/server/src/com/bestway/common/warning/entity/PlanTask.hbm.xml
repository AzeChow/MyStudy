<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.common.warning.entity.PlanTask" table="PlanTask">
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="itemName" />
		<property name="timestamp" column="timepoint" />		
		<property name="excuteKind" />		
		<property name="excuteday" />	
		<property name="excutetime" />	
		<property name="note" />	
				
		<property name="content1" length="225"/>
		<property name="content2" length="225"/>	
		<property name="content3" length="225"/>	
		<property name="content4" length="225"/>	
		<property name="content5" length="225"/>
		
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="PlanTask_company"/>
		<many-to-one lazy="false"      name="aclUser" column="aclUserId" 
			class="com.bestway.common.authority.entity.AclUser"/>
		
	</class>
</hibernate-mapping>