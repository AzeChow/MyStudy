<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.common.warning.entity.WarningThread" table="WarningThread">
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="type" />
		<property name="isStart" />		
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="warningThread_company"/>
		
	</class>
</hibernate-mapping>