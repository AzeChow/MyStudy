<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class 
		name="com.bestway.common.fpt.entity.FptAppItem" 
		table="fptAppItem">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="inOutFlag"/>
		<property name="modifyMarkState"/>
		<many-to-one lazy="false"      name="fptAppHead" column="cERBillId" 
			class="com.bestway.common.fpt.entity.FptAppHead"/>
		<property name="listNo"/>
		<property name="inOutNo"/>
		<property name="inEmsNo"/>
		<property name="trNo"/>
		<many-to-one lazy="false"      name="codeTs" column="codeTsId" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex"/>			
		<property name="name"/>
		<property name="spec"/>
		<many-to-one lazy="false"      name="unit" column="unitId" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false"      name="unit1" column="unitId1" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<property name="qty"/>
		<property name="unitPrice"/>
		<property name="qty1"/>
		<many-to-one lazy="false"      name="standComplex" column="standComplexId" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex"/>
		<many-to-one lazy="false"      name="curr" column="currId" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>			
			
		<property name="note"/>
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="cercommodityinfo_company"/>
	</class>
</hibernate-mapping>