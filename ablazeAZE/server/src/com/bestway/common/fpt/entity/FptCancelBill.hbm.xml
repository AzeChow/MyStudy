<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  
		name="com.bestway.common.fpt.entity.FptCancelBill" 
		table="fptCancelBill">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="sysType"/>
		<property name="billSort"/>
		<property name="appNo"/>
		<property name="copNo"/>
		<property name="seqNo"/>
		<property name="tradeCode" />
		<property name="billNo" />
		<property name="canelSort" />
		<property name="declareState" />
		<property name="note" />
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="transraconitbl_company"/>
	</class>
</hibernate-mapping>