<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  
		name="com.bestway.common.fpt.entity.FptInitBillItem" 
		table="fptinitbillitem">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="emsNo" />
		<property name="seqNum" />
		<many-to-one lazy="false"      name="complex" column="complex" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex"/>
		<property name="commName" />
		<property name="commSpec" />
		<many-to-one lazy="false"      name="unit" column="unitid" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<!--<many-to-one lazy="false"      name="materiel" column="materielId" 
			class="com.bestway.common.materialbase.entity.Materiel" index="initllcootyinfo_Materiel"/>-->
		<many-to-one lazy="false"      name="fptInitBill" 
			column="fptInitBill" 
			class="com.bestway.common.fpt.entity.FptInitBill"/>
		<property name="noTransferQuantity" />
		<property name="memo" />
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="initllcootyinfo_company"/>
	</class>
</hibernate-mapping>