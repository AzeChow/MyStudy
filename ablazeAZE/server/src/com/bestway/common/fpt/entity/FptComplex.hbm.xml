<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class lazy="false" name="com.bestway.common.fpt.entity.FptComplex" 
		table="fptAppHead">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<many-to-one lazy="false" name="scmCoc" column="scmCocId" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>
		<property name="name"/>
		<property name="code"/>
		<property name="spec"/>
		<many-to-one lazy="false" name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" 
			index="fptComplex_index_company"/>
	</class>
</hibernate-mapping>