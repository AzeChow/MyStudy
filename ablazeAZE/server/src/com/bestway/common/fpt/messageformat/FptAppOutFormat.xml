<?xml version="1.0" encoding="UTF-8"?>
<message version="1.0" type="FptAppOut">
	<description>结转申请表报文定义及获取方法描述</description>
	<section-flag>FPT_DATA</section-flag>
	<sub-section>
		<description>签名信息</description>
		<file-name>FptAppSignFormat.xml</file-name>
	</sub-section>
	
	<section multiple="false" required="true">
		<description>转出结转申请表表头</description>
		<section-flag>FPT_APP_OUT_HEAD</section-flag>
		<data-id>FptAppHead</data-id>
		<field nullable="true">
			<description>申请表编号</description>
			<field-name>appNo</field-name>
			<format>X(12)</format>
			<field-flag>APP_NO</field-flag>
		</field>
		<field nullable="true">
			<description>电子口岸统一编号</description>
			<field-name>seqNo</field-name>
			<format>X(18)</format>
			<field-flag>SEQ_NO</field-flag>
		</field>
		<field nullable="false">
			<description>申请表类型</description>
			<field-name>appClass</field-name>
			<format>X</format>
			<field-flag>APP_CLASS</field-flag>
		</field>
		<field nullable="false">
			<description>转出企业内部编号</description>
			<field-name>outCopAppNo</field-name>
			<format>X(20)</format>
			<field-flag>COP_APP_NO</field-flag>
		</field>
		<field nullable="false">
			<description>转出地海关</description>
			<field-name>outCustoms.code</field-name>
			<format>X(4)</format>
			<field-flag>MAST_CUST</field-flag>
		</field>
		<field nullable="false">
			<description>转出手册/账册编号</description>
			<field-name>emsNo</field-name>
			<format>X(12)</format>
			<field-flag>EMS_NO</field-flag>
		</field>
		<field nullable="false">
			<description>转出企业代码</description>
			<field-name>outTradeCode</field-name>
			<format>X(10)</format>
			<field-flag>TRADE_CODE</field-flag>
		</field>
		<field nullable="false">
			<description>转出企业名称</description>
			<field-name>outTradeName</field-name>
			<format>X(30)</format>
			<field-flag>TRADE_NAME</field-flag>
		</field>
		<field nullable="false">
			<description>申报企业9位组织机构代码</description>
			<field-name>outAgentCode</field-name>
			<format>X(10)</format>
			<field-flag>AGENT_CODE</field-flag>
		</field>
		<field nullable="false">
			<description>申报企业组织机构名称</description>
			<field-name>outAgentName</field-name>
			<format>X(30)</format>
			<field-flag>AGENT_NAME</field-flag>
		</field>
		<field nullable="true">
			<description>转出企业法人/联系电话</description>
			<field-name>outCorp</field-name>
			<format>X(32)</format>
			<field-flag>CORP</field-flag>
		</field>
		<field nullable="true">
			<description>转出申报人/联系电话</description>
			<field-name>outDecl</field-name>
			<format>X(32)</format>
			<field-flag>DECL</field-flag>
		</field>
		<field nullable="false">
			<description>转出地</description>
			<field-name>outDistrict.code</field-name>
			<format>X(5)</format>
			<field-flag>DIST_CODE</field-flag>
		</field>
		<field nullable="true">
			<description>企业合同号</description>
			<field-name>contrNo</field-name>
			<format>X(32)</format>
			<field-flag>CONTR_NO</field-flag>
		</field>
		<field nullable="false">
			<description>转出企业批准证编号</description>
			<field-name>outLiceNo</field-name>
			<format>X(20)</format>
			<field-flag>LICE_NO</field-flag>
		</field>
		<field nullable="false">
			<description>转出申报日期</description>
			<field-name>outDate</field-name>
			<format>Z(8)</format>
			<field-flag>D_DATE</field-flag>
		</field>
		<field nullable="true">
			<description>转出企业备注</description>
			<field-name>outNote</field-name>
			<format>X(50)</format>
			<field-flag>NOTE</field-flag>
		</field>
		<field nullable="false">
			<description>转入企业代码</description>
			<field-name>inTradeCode</field-name>
			<format>X(10)</format>
			<field-flag>IN_TRADE_CODE</field-flag>
		</field>
		<field nullable="false">
			<description>转入企业名称</description>
			<field-name>inTradeName</field-name>
			<format>X(30)</format>
			<field-flag>IN_TRADE_NAME</field-flag>
		</field>
		<field nullable="false">
			<description>目的地</description>
			<field-name>inDistrict.code</field-name>
			<format>X(5)</format>
			<field-flag>IN_DIST_CODE</field-flag>
		</field>
		<field nullable="true">
			<description>预计运输耗时（天）</description>
			<field-name>conveyDay</field-name>
			<format>Z(12)9.9(5)</format>
			<field-flag>CONVEY_DAY</field-flag>
		</field>
		<field nullable="true">
			<description>送货距离（公里）</description>
			<field-name>conveySpa</field-name>
			<format>Z(12)9.9(5)</format>
			<field-flag>CONVEY_SPA</field-flag>
		</field>
		<field nullable="false">
			<description>申报类型</description>
			<field-name>delcareType</field-name>
			<format>X(1)</format>
			<field-flag>DECLARE_TYPE</field-flag>
		</field>
		<field nullable="false">
			<description>联网监管企业标记</description>
			<field-name>lwMark</field-name>
			<format>X(1)</format>
			<field-flag>LW_MARK</field-flag>
		</field>
		<field nullable="true">
			<description>申请表有效期</description>
			<field-name>endDate</field-name>
			<format>Z(8)</format>
			<field-flag>END_DATE</field-flag>
		</field>
	</section>
	<section multiple="false" required="true">
		<description>结转申请表表体</description>
		<section-flag>FPT_APP_LIST</section-flag>
		<sub-section>
			<description>结转申请表表体明细</description>
			<file-name>FptAppItemFormat.xml</file-name>
		</sub-section>
	</section>
</message>
