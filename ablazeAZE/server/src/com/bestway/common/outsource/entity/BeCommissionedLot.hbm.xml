<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class 
		name="com.bestway.common.outsource.entity.BeCommissionedLot" 
		table="becommissionedlot">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="billCode" />
		<many-to-one lazy="false"      name="scmCoc" column="scmcoc" 
			class="com.bestway.common.materialbase.entity.ScmCoc"/>
		<many-to-one lazy="false"      name="customs" column="customs" 
			class="com.bestway.bcus.custombase.entity.countrycode.Customs"/>
		<property name="beCommissionedDate" />
		<property name="planCompleteDate" />
		<property name="requestDate"/>
		<property name="approveDate"/>
		<property name="billState" />
		<property name="creater"/>
		<property name="memo"/>
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="becommiodlot_company"/>	
	</class>
</hibernate-mapping>