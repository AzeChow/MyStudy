<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.common.tools.entity.FtpConfig" 
		table="FtpConfig">
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="server"/>
		<property name="password"/>
		<property name="userName"/>
		<property name="remoteDir"/>
		<property name="prefixReportName"/>		
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company"/>
	</class>
</hibernate-mapping>
<!-- parsed in 0ms -->

		