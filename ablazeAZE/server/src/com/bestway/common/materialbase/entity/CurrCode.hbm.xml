<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.materialbase.entity.CurrCode" table="currcode">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
       	<property name="code" length="10"/>
        <property name="name" />
		<property name="currSign" length="10"/>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="currcode"/>
		<many-to-one lazy="false"      name="curr" column="CurrCode" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
    </class>

</hibernate-mapping>
