<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.materialbase.entity.EnterpriseBomMaster" table="enterprisebommaster">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="ptNo" index="ebommaster_ptno"/>
		<property name="ptName"/>
		<property name="ptSpec"/>
		<property name="calUnitName"/>
		<!--<many-to-one lazy="false"  name="calUnit" column="ptunit" 
			class="com.bestway.common.materialbase.entity.CalUnit"/>-->
		<many-to-one lazy="false"  name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="ebommaster_coid"/>
    </class>
</hibernate-mapping>
