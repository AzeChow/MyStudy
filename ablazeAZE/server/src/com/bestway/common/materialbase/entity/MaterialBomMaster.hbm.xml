<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.materialbase.entity.MaterialBomMaster" table="materialbommaster">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<many-to-one lazy="false"      name="materiel" column="materiel" 
			class="com.bestway.common.materialbase.entity.Materiel" index="mbmaster_materiel"/>
		<!--<property name="ptNo"/>-->
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="mbmaster_coid"/>
		<property name="stateMark"/>	
    </class>

</hibernate-mapping>
