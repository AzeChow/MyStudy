<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.common.materialbase.entity.SysArea" table="sysarea">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="code" length="10"/>
		<property name="name" />
		<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="sysarea_company"/>
		<many-to-one lazy="false"      name="country" column="countryCode" 
			class="com.bestway.bcus.custombase.entity.countrycode.Country"/>
	</class>

</hibernate-mapping>
