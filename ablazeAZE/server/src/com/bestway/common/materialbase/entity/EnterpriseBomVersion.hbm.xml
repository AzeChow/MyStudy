<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.materialbase.entity.EnterpriseBomVersion" table="entprisebomversion">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<!--<many-to-one lazy="false"      name="master" column="masterId" 
			class="com.bestway.common.materialbase.entity.EnterpriseBomMaster"/>-->
		<property name="parentNo"/>
		<property name="version" length="1" index="ebomv_version"/>	
		<property name="beginDate"/>
		<property name="endDate"/>
		<property name="notes"/>
		<property name="netWeight"/>
		<many-to-one lazy="false"  name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company" index="ebomv_coid"/>
    </class>

</hibernate-mapping>
