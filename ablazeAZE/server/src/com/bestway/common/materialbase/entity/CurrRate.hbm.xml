<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.materialbase.entity.CurrRate" table="currrate">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="currrate_company"/>
		<many-to-one lazy="false"      name="curr" column="curr" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
		<many-to-one lazy="false"      name="curr1" column="curr1" 
			class="com.bestway.bcus.custombase.entity.parametercode.Curr"/>
		<property name="rate" />
		<property name="createDate" />
    </class>

</hibernate-mapping>
