<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.materialbase.entity.EnterpriseBomManage" table="bommanage">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="parentNo" index="bommanage_parentno"/>
		<property name="compentNo" index="bommanage_compentno"/>
		<property name="unitDosage" />
		<property name="unitWare" />
		<property name="ware" />
		<property name="netWeight" />
		<property name="grossWeight" />
		<property name="isMainImg" />
		<property name="effectDate" />
		<property name="endDate"/>
		<property name="wasterRate"/>
		<property name="versionNo" index="bommanage_versionNo"/>
		<property name="wareRate"/>
		<property name="wasterWeight"/>
		<property name="imgSource"/>		
		<property name="childEffectDate"/>
		<property name="childEndDate"/>
		<property name="childVersionNo" index="bommanage_cvNo"/>
		<property name="memo"/>
		<many-to-one lazy="false" column="coid" name="company" class="com.bestway.bcus.system.entity.Company" index="bommanage_coid"/>
    </class>

</hibernate-mapping>
