<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.common.authority.entity.AclGroupUser" table="aclgroupuser" optimistic-lock="version">
		<cache usage="read-write"/>
		<id name="id" column="id" length = "32">
			<generator class="uuid.hex"/>
		</id>
        <many-to-one lazy="false"      name="aclGroup" column="groupid" class="com.bestway.common.authority.entity.AclGroup"/>
		<many-to-one lazy="false"      name="aclUser" column="userid" class="com.bestway.common.authority.entity.AclUser"/>
		<many-to-one lazy="false"      name="company" column="companyId" class="com.bestway.bcus.system.entity.Company"/>
	</class>
</hibernate-mapping>
