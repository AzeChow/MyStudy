<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.common.authority.entity.AclUser" table="acluser" optimistic-lock="version">
		<cache usage="read-write"/>
		<id name="id" column="id" length = "32">
			<generator class="uuid.hex"/>
		</id>
		<property name="createDate"/>
		<property name="email"/>
		<property name="lastlogin"/>
		<property name="loginName"/>
		<property name="userName"/>
		<property name="modified"/>
		<property name="password"/>
		<property name="userFlag" length="10"/>
		<property name="style"/>
		<property name="isMenuNavigation"/>		
		<property name="isForbid"/>		
		<many-to-one lazy="false"      name="company" column="companyId" class="com.bestway.bcus.system.entity.Company"/>
	</class>
</hibernate-mapping>
