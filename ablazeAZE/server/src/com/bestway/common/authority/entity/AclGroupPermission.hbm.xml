<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.common.authority.entity.AclGroupPermission" table="aclgrouppermission">
		<cache usage="read-write"/> 
       	<id name="id" column="id" length = "32">
			<generator class="uuid.hex"/>
	</id>
        <property name="permission"/>
        <property name="moduleName"/>
        <many-to-one lazy="false"      name="group" column="groupId" class= "com.bestway.common.authority.entity.AclGroup"/>
				<many-to-one lazy="false"      name="company" column="companyId" class="com.bestway.bcus.system.entity.Company"/>
    </class>

</hibernate-mapping>
