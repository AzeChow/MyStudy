<?xml version="1.0" encoding="UTF-8"?>
<fileformat>
	<sheetformat class="com.bestway.bcs.dictpor.entity.BcsDictPorHead" 
		sheetindex="1" multi-row="false" desc="备案资料库-表头">
		<field column="2" row="3" key="true" desc="内部编号">copEmsNo</field>
		<field column="4" row="3" >dictPorEmsNo</field>
		<field column="6" row="3" >tradeCode</field>
		<field column="7" row="3" >tradeName</field>
		<field column="6" row="3" >machCode</field>
		<field column="7" row="3" >machName</field>
		<field column="2" row="4" >productRatio</field>
		<field column="4" row="4" refProperty="name">declareCustoms</field>
		<field column="6" row="4">declareDate</field>
		<field column="2" row="5">memo</field>
	</sheetformat>
	<sheetformat class="com.bestway.bcs.dictpor.entity.BcsDictPorImg" 
		sheetindex="2" multi-row="true" beginRow="3" desc="备案资料库-料件">
		<field column="1" key="true">seqNum</field>
		<field column="2"></field>
		<field column="3+4" refProperty="code">complex</field>
		<field column="5">commName</field>
		<field column="6" refProperty="name">comUnit</field>
		<field column="7" valueMap="主料=true">isMainImg</field>
		<field column="8">commSpec</field>
		<field column="9"></field>
		<field column="10">unitPrice</field>
		<field column="11" refProperty="name">curr</field>
		<field column="12" valueMap="不变=0/修改=1/新增=3/删除=2">modifyMark</field>
		<field column="13">memo</field>
		<field>dictPorHead</field>
	</sheetformat>
	<sheetformat class="com.bestway.bcs.dictpor.entity.BcsDictPorExg" 
		sheetindex="3" multi-row="true" beginRow="3" desc="备案资料库-成品">
		<field column="1" key="true">seqNum</field>
		<field column="2"></field>
		<field column="3+4" refProperty="code">complex</field>
		<field column="5">commName</field>
		<field column="6" refProperty="name">comUnit</field>
		<field column="7"></field>
		<field column="8">commSpec</field>
		<field column="9">unitPrice</field>
		<field column="10" refProperty="name">curr</field>
		<field column="11" valueMap="不变=0/修改=1/新增=3/删除=2">modifyMark</field>
		<field column="12">memo</field>
		<field>dictPorHead</field>
	</sheetformat>
</fileformat>