<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" 
		name="com.bestway.common.erpbill.entity.OrderDateIndex" 
		table="orderDateIndex">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
	
		<property name="scmCoc"/>
		<property name="billCode"/>
		<property name="ifzc"/>
		<property name="orderDate"/>
		<property name="ptNo"/>
		<property name="bgAmount"/>
		<property name="bgUnit"/>
		<property name="salesDate"/>
		<property name="state"/>
		<property name="orderERPId"/>
		
		<many-to-one lazy="false"      name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="dzsccc_coid"/>
		
	</class>

</hibernate-mapping>
