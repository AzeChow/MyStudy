<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.common.erpbill.entity.CustomOrderBom"
		table="customOrderBom">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>

		<many-to-one lazy="false" name="customOrderExg" column="customOrderExgid"
			class="com.bestway.common.erpbill.entity.CustomOrderExg" index="customOrderExg_Bom" />
		<property name="seqNum" />
		<many-to-one lazy="false" name="complex" column="complex"
			class="com.bestway.bcus.custombase.entity.hscode.Complex" index="contract_complex" />
		<!--
			<many-to-one lazy="false" name="customOrderDetail"
			column="customOrderDetailid"
			class="com.bestway.common.erpbill.entity.CustomOrderDetail"
			index="customOrderDetail_Bom"/>
		-->
		<property name="ptNo" />
		<property name="name" />
		<property name="spec" />
		<many-to-one lazy="false" name="unit" column="unit"
			class="com.bestway.bcus.custombase.entity.parametercode.Unit" />
		<property name="declarePrice" />
		<property name="note" />
		<property name="amount" />
		<property name="unitWaste" />
		<property name="waste" column="wasteAmount" />
		<property name="unitPrice" />
		<property name="totalPrice" />
		<property name="legalAmount" />
		<property name="unitDosage" />
		<property name="contractImgSeqNum" />
		<property name="contractImgId" />
		<!--
			<many-to-one lazy="false" name="legalUnit" column="legalUnit"
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		-->
		<property name="unitWeight" />
		<property name="isMainImg" />
		<many-to-one lazy="false" name="country" column="country"
			class="com.bestway.bcus.custombase.entity.countrycode.Country" />
		<many-to-one lazy="false" name="company" column="coid"
			class="com.bestway.bcus.system.entity.Company" index="contract_coid" />
		<!--<property name="declareState"/>-->
		<many-to-one lazy="false" name="curr" column="curr"
			class="com.bestway.bcus.custombase.entity.parametercode.Curr" />
		<property name="imgCredenceNo" />
		<property name="createDate" />
		<property name="modifyDate" />
		<property name="modifyMark" />
		<property name="transNum" />
		<property name="contractNum" />
		<property name="notTransNum" />
		<property name="notContractNum" />
	</class>
</hibernate-mapping>
	<!-- parsed in 0ms -->