<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class 
		name="com.bestway.common.erpbill.entity.MakeCustomOrderToContract" 
		table="MakeCustomOrderToContract">
		<cache usage="read-write"/><id name="id" column="id" length="32"><generator class="uuid.hex"/></id>
		<property name="projectType" />
		<property name="contractImgExgId" />
		<property name="customOrderDetailId" />
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company"/>
	</class>
</hibernate-mapping>