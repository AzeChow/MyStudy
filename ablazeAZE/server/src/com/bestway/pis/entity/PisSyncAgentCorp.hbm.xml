<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.pis.entity.PisSyncAgentCorp"
		table="pissyncagentcorp">
		<cache usage="read-write" />
		<id name="id" column="id">
			<generator class="uuid.hex" />
		</id>

		<property name="info" />
		<property name="syncState" />
		<property name="briefCode" />
		<property name="briefName" />
		<property name="linkMan" />
		<property name="linkTel" />
		<property name="address" />

		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="pissyncagentcorp_company" />
	</class>

</hibernate-mapping>
