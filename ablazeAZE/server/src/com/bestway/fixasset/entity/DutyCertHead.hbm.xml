<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fixasset.entity.DutyCertHead" table="dutycerthead">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="sancEmsNo"/>
		<property name="certNo"/>
		<property name="certApplyDate"/>
		<property name="certApproveDate"/>
		<property name="certEffectDate"/>	
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="fecavbill_2"/>	
		</many-to-one>
    </class>

</hibernate-mapping>
