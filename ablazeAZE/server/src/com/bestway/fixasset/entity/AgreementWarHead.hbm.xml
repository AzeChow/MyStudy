<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fixasset.entity.AgreementWarHead" table="agreementwarhead">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="sancEmsNo"/>
		<property name="warNo"/>
		<property name="applier"/>
		<property name="linkMan"/>
		<property name="linkTel"/>
		<property name="address"/>
		
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="fecavbill_2"/>	
		</many-to-one>
    </class>

</hibernate-mapping>
