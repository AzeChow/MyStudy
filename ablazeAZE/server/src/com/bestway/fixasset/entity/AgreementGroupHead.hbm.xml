<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fixasset.entity.AgreementGroupHead" table="agreementgrouphead">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<many-to-one lazy="false"  name="agreement" column="agreement" 
			class="com.bestway.fixasset.entity.Agreement"/>		
		<property name="changedTimes"/>
		<property name="groupNo"/>
		<property name="invoiceNo"/>
		<property name="certNo"/>
		<property name="warNo"/>
		<property name="stateMark"/>
		<property name="createDate"/>
		
		<property name="dutyCertApplyDate"/>
		<property name="dutyCertApproveDate"/>
		<property name="prepExportNoteDate"/>
		<property name="arriveHKDate"/>
		<property name="arriveDGDate"/>
		<property name="inspectDate"/>
		<property name="declareDate"/>
		<property name="inFactDate"/>
		
		<property name="applier"/>
		<property name="customsDeclarationCode"/>
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="fecavbill_2"/>	
		</many-to-one>
    </class>

</hibernate-mapping>
