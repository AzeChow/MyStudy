<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fixasset.entity.AgreementInvoiceHead" table="agreementinvoicehead">
		<cache usage="read-write"/> 
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<property name="sancEmsNo"/>
		<property name="invoiceCode"/>
		<property name="sender"/>
		<property name="senderAddr"/>
		<property name="senderContactPerson"/>
		<property name="senderTel"/>
		
		<property name="receiver"/>
		<property name="receiverAddr"/>		
		<property name="receiverContactPerson"/>
		<property name="receiverTel"/>
		
		<property name="consigner"/>
		<property name="consignerAddr"/>
		<property name="consignerContactPerson"/>
		<property name="consignerTel"/>
		<property name="fromPlace"/>
		<property name="toPlace"/>
		<property name="transporter"/>
		<property name="transTime"/>
		<property name="invoiceDate"/>
		<many-to-one lazy="false"  name="transMode" column="transMode" 
			class="com.bestway.bcus.custombase.entity.parametercode.Transf"/>
		<property name="lc"/>
		<property name="accBank"/>
		<property name="note"/>
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="fecavbill_2"/>	
		</many-to-one>
    </class>

</hibernate-mapping>
