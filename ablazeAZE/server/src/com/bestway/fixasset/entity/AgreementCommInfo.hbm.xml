<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.fixasset.entity.AgreementCommInfo" table="agreementcomminfo">
		<cache usage="read-write"/> 		
        <id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
	    </id>
		<many-to-one lazy="false"  name="agreement" column="agreement" 
			class="com.bestway.fixasset.entity.Agreement"/>
		<property name="mainNo"/>
		<many-to-one lazy="false"  name="complex" column="complex" 
			class="com.bestway.bcus.custombase.entity.hscode.Complex"/>
		<property name="commName"/>
		<property name="commSpec"/>
		<property name="unitPrice"/>
		<property name="amount"/>
		<property name="totalPrice"/>
		<property name="impedAmount"/>
		<property name="impedTotalPrice"/>
		<property name="stateMark"/>
		<many-to-one lazy="false"  name="unit" column="unit" 
			class="com.bestway.bcus.custombase.entity.parametercode.Unit"/>
		<many-to-one lazy="false"  name="country" column="country" 
			class="com.bestway.bcus.custombase.entity.countrycode.Country"/>		
		<many-to-one lazy="false" name="company" class="com.bestway.bcus.system.entity.Company">
			<column name="coid" index="fecavbill_2"/>	
		</many-to-one>
    </class>
</hibernate-mapping>
