<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bls.entity.BlsAutoBackBillInfo" 
		table="blsautobackbillinfo">
		<cache usage="read-write"/>
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="serviceType"/>
		<property name="relateId"/>
		<property name="inOut"/>
		<property name="billCode"/>
		<property name="declareTimes"/>
		<property name="declareDates"/>
		
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company"/>
	</class>
</hibernate-mapping>