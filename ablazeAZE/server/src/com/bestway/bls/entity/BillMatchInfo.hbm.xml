<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class lazy="false"  name="com.bestway.bls.entity.BillMatchInfo" table="billMatchInfo">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>
		<property name="inBillNo" />
		<property name="outBillNo" />
		<property name="matchNo" />
		<property name="outMatchNo" />
		<property name="ptNo" />
		<property name="matchName" />
		<property name="matchSpec" />
		<property name="unitName" />
		<property name="customerName" />
		<property name="oldInBillNo" />
		<property name="oldInBillGoodNo" />
		<property name="detailQty" />
		<property name="matchDate" />				
		<many-to-one lazy="false" name="company" column="coid"
			class="com.bestway.bcus.system.entity.Company" />
	</class>
</hibernate-mapping>