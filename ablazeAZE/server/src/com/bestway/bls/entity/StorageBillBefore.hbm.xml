<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bls.entity.StorageBillBefore"
		table="storageBillBefore">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>

		<property name="subpoenaNo"></property>
		<property name="partNameE"></property>
		<property name="partNameC"></property>
		<property name="clor"></property>
		<property name="detailQty"></property>

		<many-to-one lazy="false" name="storageBillAfter"
			class="com.bestway.bls.entity.StorageBillAfter" />

		<many-to-one lazy="false" name="partNo"
			class="com.bestway.common.materialbase.entity.Materiel" />

		<many-to-one lazy="false" name="company" column="coid"
			class="com.bestway.bcus.system.entity.Company" />

	</class>
</hibernate-mapping>