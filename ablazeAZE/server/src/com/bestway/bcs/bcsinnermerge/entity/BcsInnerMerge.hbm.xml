<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class  lazy="false"  name="com.bestway.bcs.bcsinnermerge.entity.BcsInnerMerge" table="bcsInnerMerge">
		<!-- <cache usage="read-write"/> -->
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>		
	
		<many-to-one lazy="false" name="materiel" column="materielid" 
			class="com.bestway.common.materialbase.entity.Materiel" index="bcsInnerMerge_materiel"/>	
		
		<many-to-one lazy="false" name="company" column="coid" 
			class="com.bestway.bcus.system.entity.Company" index="bcsInnerMerge_coid"/>	
			
		<many-to-one lazy="false" name="bcsTenInnerMerge" column="bcsTenInnerMerge" 
			class="com.bestway.bcs.bcsinnermerge.entity.BcsTenInnerMerge" index="bcsInnerMerge_tenMerge"/>
			
				
		<property name="materielType"/>	
		<property name="unitConvert"/>	
		<property name="isUsing"/>	
		<property name="createDate"/>
	</class>
</hibernate-mapping>

