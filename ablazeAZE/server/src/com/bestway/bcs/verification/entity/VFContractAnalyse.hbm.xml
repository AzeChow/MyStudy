<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcs.verification.entity.VFContractAnalyse"
		table="vfcontractanalyse">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="serialNumber" />
		<property name="mergerNo" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnit" />
		<property name="unitPrice" />
		<property name="impAmount" not-null="true"/>
		<property name="expAmount" not-null="true"/>
		<property name="contractLeaveNum" not-null="true"/>
		<property name="stockAmount" />
		<property name="overflowAmount" />
		<property name="deficitAmount" />
		<property name="overflowPrice" />
		<property name="deficitPrice" />
		<property name="emsNo" />
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcs.verification.entity.VFSection" index="vfcustomscountexgconvert_section"/>
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="vfcustomscountexgconvert_company" />
	</class>
</hibernate-mapping>
	<!-- parsed in 0ms -->