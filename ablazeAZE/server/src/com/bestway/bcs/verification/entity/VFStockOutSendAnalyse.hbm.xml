<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcs.verification.entity.VFStockOutSendAnalyse"
		table="vfstockoutsendanalyse">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="imgHsAmount" />
		<property name="exgHsAmount" />
		<property name="semiExgHsAmount" />
		<property name="hsAmount" />
		<property name="mergerNo" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnit" />
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcs.verification.entity.VFSection" index="vfstockoutsendanalyse_section"/>
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="vfstockoutsendanalyse_company" />
	</class>
</hibernate-mapping>
