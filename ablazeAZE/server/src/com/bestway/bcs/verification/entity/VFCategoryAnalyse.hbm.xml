<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class lazy="false" name="com.bestway.bcs.verification.entity.VFCategoryAnalyse"
		table="VFCategoryAnalyse">
		<cache usage="read-write" />
		<id name="id" column="id" length="32">
			<generator class="uuid.hex" />
		</id>
		<property name="serialNumber" />
		<property name="seqNum" />
		<property name="mergerNo" />
		<property name="hsName" />
		<property name="hsSpec" />
		<property name="hsUnit" />
		<property name="impAmount" />
		<property name="expAmount" />
		<property name="contractLeaveNum" />
		<property name="stockAmountImg" />
		<property name="stockAmountProcessImg" />
		<property name="stockAmountExg" />
		<property name="stockAmountOutSend" />
		<property name="stockAmountOutFactory" />
		<property name="stockAmountBuy" />
		<property name="stockAmountTraveling" />
		<property name="stockTotalAmount" />
		<property name="unReceiveHadTransNum" />
		<property name="unTransHadSendNum" />
		<property name="unSendHadTransNum" />
		<property name="unTransHadReceiveNum" />
		<property name="transferDiffNum" />
		<property name="overOrshortNum" />
		<many-to-one lazy="false" name="section" column="section"
			class="com.bestway.bcs.verification.entity.VFSection" index="vfanalyse_section"/>
		<many-to-one lazy="false" name="company" column="company"
			class="com.bestway.bcus.system.entity.Company" index="vfanalyse_company" />
	</class>
</hibernate-mapping>
