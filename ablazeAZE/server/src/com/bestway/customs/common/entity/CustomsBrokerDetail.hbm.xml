<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class  lazy="false"  name="com.bestway.customs.common.entity.CustomsBrokerDetail" 
    	table="CustomsBrokerDetail">
		<cache usage="read-write"/> 
		<id name="id" column="id" length="32">
			<generator class="uuid.hex"/>
		</id>	
		<property name="customDeclarationId" />
        <property name="projectType" />
        <property name="declarationDate" />
		<property name="customsBrokerCode"/>
		<property name="customsBrokerName"/>
		<property name="impExpFlag"/>
		<property name="tradeMode"/>
		<property name="customserName"/>
		<property name="emsNo"/>
				<many-to-one lazy="false"      name="company" column="companyId" 
			class="com.bestway.bcus.system.entity.Company"/>	
    </class>
</hibernate-mapping>